LOAD DATABASE
     FROM {{SOURCE_URI}}
     INTO {{TARGET_URI}}

WITH
     schema only,
     include no drop,
     create tables,
     create indexes,
     reset sequences,
     foreign keys,
     workers = 1,
     concurrency = 1

ALTER TABLE NAMES MATCHING ~/.*/ SET SCHEMA 'public'

CAST
     type tinyint to smallint drop typemod,
     type datetime to timestamp;
