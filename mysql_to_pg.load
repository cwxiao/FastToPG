LOAD DATABASE
     FROM {{SOURCE_URI}}
     INTO {{TARGET_URI}}

WITH
     include drop,
     create tables,
     create indexes,
     reset sequences,
     foreign keys,
     workers = 2,
     concurrency = 1,
     batch rows = 10000,
     prefetch rows = 10000

CAST
     type tinyint to smallint drop typemod,
     type datetime to timestamp;
