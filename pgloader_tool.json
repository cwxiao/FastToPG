{
  "databases": [
    "iucp_camunda",
    "iucp_data",
    "iucp_nacos",
    "iucp_normal",
    "iucp_xxl_job"
  ],
  "load_template": "mysql_to_pg.load",
  "source": {
    "type": "mysql",
    "uri": "mysql://root:123456@host.docker.internal:3306/{{DB_NAME}}"
  },
  "target": {
    "uri": "postgresql://postgres:Postgres123!@host.docker.internal:5432/{{DB_NAME}}"
  },
  "mysql": {
    "container": "mysql8",
    "user": "root",
    "password": "123456"
  },
  "pgloader": {
    "image": "dimitri/pgloader",
    "env": {
      "PGLOADER_DYNSIZE": "2GB",
      "SBCL_DYNAMIC_SPACE_SIZE": "4096"
    },
    "show_output": false
  }
}
